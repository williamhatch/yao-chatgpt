{
    "name": "::AI Model",
    "table": {
        "name": "ai_model",
        "label": "AI模型列表"
    },
    "columns": [
        {
            "name": "id",
            "label": "ID",
            "type": "ID"
        },
        {
            "name": "idx",
            "label": "模型标识",
            "type": "string",
            "nullable": false,
            "unique": true,
            "index": true
        },
        {
            "name": "title",
            "label": "标题",
            "type": "string"
        },
        {
            "name": "Remark",
            "label": "备注",
            "type": "string"
        },
        {
            "name": "score",
            "label": "评分",
            "type": "integer"
        },
        {
            "name": "object",
            "label": "类型",
            "type": "string",
            "nullable": false
        },
        {
            "label": "状态",
            "comment": "Status",
            "name": "status",
            "type": "enum",
            "default": "enabled",
            "option": [
                "enabled",
                "disabled"
            ],
            "index": true,
            "validations": [
                {
                    "method": "typeof",
                    "args": [
                        "string"
                    ],
                    "message": "{{input}} Error, {{label}} should be string"
                },
                {
                    "method": "enum",
                    "args": [
                        "enabled",
                        "disabled"
                    ],
                    "message": "{{input}} Error, {{label}} should be enabled/disabled"
                }
            ]
        },
        {
            "name": "owned_by",
            "label": "拥有者",
            "type": "string",
            "nullable": false
        },
        {
            "name": "created",
            "label": "创建时间",
            "type": "datetime"
        },
        {
            "name": "parent",
            "label": "上级",
            "type": "string"
        },
        {
            "name": "root",
            "label": "根节点",
            "type": "string"
        }
    ],
    "relations": {
        "permissions": {
            "type": "hasMany",
            "model": "ai.permission",
            "key": "model_id",
            "foreign": "idx"
        }
    },
    "values": [],
    "indexes": [],
    "option": {
        "timestamps": false,
        "soft_deletes": false
    }
}